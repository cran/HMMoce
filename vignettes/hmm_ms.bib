keywords = {Albatross,GPS,Generalized additive model,Geolocator,Platform transmitter terminal,Satellite tracking,Spatial models,State space models},
pages = {165--182},
title = {{Quantifying habitat use and preferences of pelagic seabirds using individual movement data: A review}},
