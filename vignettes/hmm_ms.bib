@article{Hussey2015,
abstract = {The distribution and interactions of aquatic organisms across space and time structure our marine, freshwater, and estuarine ecosystems. Over the past decade, technological advances in telemetry have transformed our ability to observe aquatic animal behavior and movement. These advances are now providing unprecedented ecological insights by connecting animal movements with measures of their physiology and environment. These developments are revolutionizing the scope and scale of questions that can be asked about the causes and consequences of movement and are redefining how we view and manage individuals, populations, and entire ecosystems. The next advance in aquatic telemetry will be the development of a global collaborative effort to facilitate infrastructure and data sharing and management over scales not previously possible.},
author = {Hussey, Nigel E and Kessel, Steven T and Aarestrup, Kim and Cooke, Steven J and Cowley, Paul D and Fisk, Aaron T and Harcourt, Robert G and Holland, Kim N and Iverson, Sara J and Kocik, John F and {Mills Flemming}, Joanna E and Whoriskey, Fred G},
