journal = {Movement Ecology},
keywords = {GPS telemetry,Geographic Information System GIS,broken stick model,habitat selection,individual modelling,remote sensing,resource selection function RSF,resource selection probability function (RSPF),state-space model,step selection function SSF},
number = {1},
pages = {26},
title = {{Applications of step-selection functions in ecology and conservation}},
url = {http://www.movementecologyjournal.com/content/2/1/4},
volume = {2},
year = {2014}
}
