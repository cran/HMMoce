file = {:Users/Cam/Documents/Mendeley Desktop/Lam, Kiefer, Domeier/2015/Lam et al 2015.pdf:pdf;:Users/Cam/Documents/Mendeley Desktop/Lam, Kiefer, Domeier/2015/mmc1.pdf:pdf;:Users/Cam/Documents/Mendeley Desktop/Lam, Kiefer, Domeier/2015/mmc2.pdf:pdf;:Users/Cam/Documents/Mendeley Desktop/Lam, Kiefer, Domeier/2015/mmc3.doc:doc},
issn = {0165-7836},
journal = {Fisheries Research},
keywords = {Dissolved oxygen,Habitat envelope,Mixed layer,Popup satellite archival tags,Thermal niche,Vertical habitat},
number = {0},
pages = {80--91},
title = {{Habitat characterization for striped marlin in the Pacific Ocean}},
